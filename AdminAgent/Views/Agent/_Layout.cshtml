                </h1>
                <ol class="breadcrumb">
                    @for (int i = 0; i < ((List<string>)ViewBag.breadcrumbOl).Count; i++)
                    {
                        if (i == 0)
                        {
                            <li><a href="#"><i class="fa fa-dashboard"></i>@(((List<string>)ViewBag.breadcrumbOl)[i])</a></li>
                        }
                        else if (i == ((List<string>)ViewBag.breadcrumbOl).Count - 1)
                        {
                            <li class="active">@(((List<string>)ViewBag.breadcrumbOl)[i])</li>
                        }
                        else
                        {
                            <li>@(((List<string>)ViewBag.breadcrumbOl)[i])</li>
                        }
                    }
                </ol>
            </section>

