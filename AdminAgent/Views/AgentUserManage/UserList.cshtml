@using Ydb.Membership.Application.Dto;
@model IDictionary<Enum_LockMemberType, IList<MemberDto>>
@{
    ViewBag.headTitle = "用户管理";
    ViewBag.subTitle = "";
    ViewBag.breadcrumbOl = new List<string>() { "用户管理", "用户列表" };
}

 <!-- Main content -->
<section class="content">
    <!-- /.row -->
    <div class="row">
        <!-- /.col -->
        <div class="col-md-12">
            <div class="nav-tabs-custom">
                <ul class="nav nav-tabs col-md-12 margin-bottom-20">
                    <li class="active"><a href="#tab_1" data-toggle="tab">活跃用户</a></li>
                    <li><a href="#tab_2" data-toggle="tab">封停记录</a></li>
                </ul>
                <div class="tab-content">
                    <div class="tab-pane active" id="tab_1">
                        <table class="table table-bordered table-striped">
                            <thead>
                                <tr>
                                    <th>序号</th>
                                    <th>用户名</th>
                                    <th>真实姓名</th>
                                    <th>电话号码</th>
                                    <th>微信账号</th>
                                    <th>QQ号码</th>
                                    <th>微博账号</th>
                                    <th>首次登录时间</th>
                                    <th>详情</th>
                                </tr>
                            </thead>
                            <tbody>
                                @for (int i = 0; i < @Model[Enum_LockMemberType.UnLocked].Count; i++)
                                {
                                    <tr>
                                        <td>@(i + 1)</td>
                                        <td>@Html.DisplayFor(modelItem => Model[Enum_LockMemberType.UnLocked][i].DisplayName)</td>
                                        <td>@Html.DisplayFor(modelItem => Model[Enum_LockMemberType.UnLocked][i].RealName)</td>
                                        <td>@Html.DisplayFor(modelItem => Model[Enum_LockMemberType.UnLocked][i].Phone)</td>
                                        <td>@Html.DisplayFor(modelItem => Model[Enum_LockMemberType.UnLocked][i].WXNumber)</td>
                                        <td>@Html.DisplayFor(modelItem => Model[Enum_LockMemberType.UnLocked][i].QQNumber)</td>
                                        <td>@Html.DisplayFor(modelItem => Model[Enum_LockMemberType.UnLocked][i].WBNumber)</td>
                                        <td>@Html.DisplayFor(modelItem => Model[Enum_LockMemberType.UnLocked][i].TimeCreated)</td>
                                        <td><a href="./UserDetail?id=@Html.DisplayFor(modelItem => Model[Enum_LockMemberType.UnLocked][i].Id).ToString()">进入</a></td>
                                    </tr>
                                }
                            </tbody>
                            <tfoot>
                                <tr>
                                    <th>序号</th>
                                    <th>用户名</th>
                                    <th>真实姓名</th>
                                    <th>电话号码</th>
                                    <th>微信账号</th>
                                    <th>QQ号码</th>
                                    <th>微博账号</th>
                                    <th>首次登录时间</th>
                                    <th>详情</th>
                                </tr>
                            </tfoot>
                        </table>
                    </div>
                    <!-- /.tab-pane -->
                    <div class="tab-pane" id="tab_2">
                        <table class="table table-bordered table-striped">
                            <thead>
                                <tr>
                                    <th>序号</th>
                                    <th>用户名</th>
                                    <th>真实姓名</th>
                                    <th>电话号码</th>
                                    <th>微信账号</th>
                                    <th>QQ号码</th>
                                    <th>微博账号</th>
                                    <th>封停时间</th>
                                    <th>详情</th>
                                </tr>
                            </thead>
                            <tbody>
                                @for (int i = 0; i < @Model[Enum_LockMemberType.Locked].Count; i++)
                                {
                                    <tr>
                                        <td>@(i + 1)</td>
                                        <td>@Html.DisplayFor(modelItem => Model[Enum_LockMemberType.Locked][i].DisplayName)</td>
                                        <td>@Html.DisplayFor(modelItem => Model[Enum_LockMemberType.Locked][i].RealName)</td>
                                        <td>@Html.DisplayFor(modelItem => Model[Enum_LockMemberType.Locked][i].Phone)</td>
                                        <td>@Html.DisplayFor(modelItem => Model[Enum_LockMemberType.Locked][i].WXNumber)</td>
                                        <td>@Html.DisplayFor(modelItem => Model[Enum_LockMemberType.Locked][i].QQNumber)</td>
                                        <td>@Html.DisplayFor(modelItem => Model[Enum_LockMemberType.Locked][i].WBNumber)</td>
                                        <td>@Html.DisplayFor(modelItem => Model[Enum_LockMemberType.Locked][i].LockTime)</td>
                                        <td><a href="./UserDetail?id=@Html.DisplayFor(modelItem => Model[Enum_LockMemberType.Locked][i].Id).ToString()">进入</a></td>
                                    </tr>
                                }
                            </tbody>
                            <tfoot>
                                <tr>
                                    <th>序号</th>
                                    <th>用户名</th>
                                    <th>真实姓名</th>
                                    <th>电话号码</th>
                                    <th>微信账号</th>
                                    <th>QQ号码</th>
                                    <th>微博账号</th>
                                    <th>封停时间</th>
                                    <th>详情</th>
                                </tr>
                            </tfoot>
                        </table>
                    </div>
                </div>
            </div>
        </div>
        <!-- /.col -->
    </div>
    <!-- /.row -->
</section>
<!-- /.content -->

@Html.Action("footer", "Agent", new { id = "" })

