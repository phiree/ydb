<UserControl x:Class="Dianzhu.CSClient.ViewWPF.UC_SearchResultOld"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Dianzhu.CSClient.ViewWPF"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <StackPanel>
            <Label x:Name="lblLoadingText"></Label>
        <ScrollViewer VerticalScrollBarVisibility="Auto" >
                
                <StackPanel  x:Name="pnlSearchResult" VerticalAlignment="Bottom">
                    
                </StackPanel>
        </ScrollViewer>
        <WrapPanel>
                <Button x:Name="btnPush" Click="btnPush_Click" IsEnabled="False">推送</Button>
        </WrapPanel>
        </StackPanel>
    </Grid>
</UserControl>
